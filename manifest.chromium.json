{
	"name": "consent-auto-rejector",
	"description": "Automatically rejects and removes cookie consent banners using rules",
	"author": "mac",
	"homepage_url": "https://github.com/macbuildssys/connsent-auto-rejector",
	"version": "REPLACED_BY_BUILD_SCRIPT",
	"manifest_version": 3,
	"permissions": [
		"activeTab",
		"tabs",
		"storage"
	],
	"host_permissions": [
		"<all_urls>"
	],
	"action": {
		"default_title": "consent-auto-rejector",
		"default_popup": "popup.html"
	},
	"background": {
		"service_worker": "service.js"
	},
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"content.js"
			],
			"css": [
				"content.css"
			],
			"all_frames": true,
			"match_about_blank": true,
			"run_at": "document_start"
		}
	],
	"options_ui": {
		"page": "options.html",
		"open_in_tab": true
	},
	"icons": {
		"48": "icon_48.png",
		"96": "icon_96.png"
	}
}