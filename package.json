{
  "name": "consent-auto-rejector",
  "version": "1.1.5",
  "description": "A browser extension that tries to fill out GDPR consent popups for you",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/macbuildssys/consent-auto-rejector"
  },
  "scripts": {
    "build-firefox": "webpack --env target=firefox",
    "build-chromium": "webpack --env target=chromium",
    "build": "webpack --env target=firefox && webpack --env target=chromium",
    "build:all": "npm run build:chromium && npm run build:firefox && npm run build:chrome"
  },
  "dependencies": {
    "copy-webpack-plugin": "^12.0.2",
    "glob": "^10.3.12",
    "sass": "^1.77.0",
    "webpack-cli": "^5.1.4",
    "zip-webpack-plugin": "^4.0.1"
  },
  "devDependencies": {
    "css-loader": "^7.1.2",
    "html-minimizer-webpack-plugin": "^5.0.3",
    "sass-loader": "^16.0.5",
    "style-loader": "^4.0.0"
  }
}
