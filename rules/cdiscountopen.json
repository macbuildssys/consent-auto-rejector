{
    "$schema": "https://raw.githubusercontent.com/cavi-au/Consent-O-Matic/master/rules.schema.json",
    "cdiscountopen": {
        "detectors": [
            {
                "presentMatcher": [
                    {
                        "type": "css",
                        "target": {
                            "selector": "#tc-privacy-wrapper"
                        }
                    }
                ],
                "showingMatcher": [
                    {
                        "type": "css",
                        "target": {
                            "selector": ".tc-privacy-iab-banner,#popin_tc_privacy",
                            "displayFilter": true
                        }
                    }
                ]
            }
        ],
        "methods": [
            {
                "action": {
                    "type": "list",
                    "actions": [
                        {
                            "type": "hide",
                            "target": {
                                "selector": "#tc-privacy-wrapper"
                            }
                        },
                        {
                            "type": "hide",
                            "target": {
                                "selector": "#privacy-overlay"
                            }
                        },
                        {
                            "type": "hide",
                            "target": {
                                "selector": "#privacy-container"
                            }
                        }
                    ]
                },
                "name": "HIDE_CMP"
            },
            {
                "action": {
                    "type": "list",
                    "actions": [
                        {
                            "type": "click",
                            "target": {
                                "selector": "#popin_tc_privacy_button,#footer_tc_privacy_button"
                            }
                        }
                    ]
                },
                "name": "OPEN_OPTIONS"
            },
            {
                "action": {
                    "type": "waitcss",
                    "target": {
                        "selector": "#tc-privacy-wrapper"
                    },
                    "retries": 100,
                    "waitTime": 250,
                    "negated": true
                },
                "name": "DO_CONSENT"
            },
            {
                "name": "SAVE_CONSENT"
            },
            {
                "name": "UTILITY"
            }
        ]
    }
}
