{
    "$schema": "https://raw.githubusercontent.com/macbuildssys/consent-auto-rejecter/master/rules.schema.json",
    "google_consentdomain_1": {
        "detectors": [
            {
                "presentMatcher": [
                    {
                        "type": "url",
                        "url": [
                            "consent.google.com"
                        ]
                    }
                ],
                "showingMatcher": [
                    {
                        "type": "css",
                        "target": {
                            "selector": ".contentText",
                            "textFilter": [
                                "cookies"
                            ]
                        }
                    }
                ]
            }
        ],
        "methods": [
            {
                "name": "HIDE_CMP"
            },
            {
                "name": "OPEN_OPTIONS"
            },
            {
                "name": "DO_CONSENT"
            },
            {
                "name": "SAVE_CONSENT"
            },
            {
                "action": {
                    "type": "click",
                    "target": {
                        "selector": ".customButtonContainer .basebutton"
                    }
                },
                "name": "UTILITY"
            }
        ]
    }
}