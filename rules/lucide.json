{
    "$schema": "https://raw.githubusercontent.com/cavi-au/Consent-O-Matic/master/rules.schema.json",
    "lucide": {
        "detectors": [
            {
                "presentMatcher": [
                    {
                        "type": "css",
                        "target": {
                            "selector": ".modal.cookie.fade.in#cookieconsent"
                        }
                    }
                ],
                "showingMatcher": [
                    {
                        "type": "css",
                        "target": {
                            "selector": ".modal.cookie.fade.in#cookieconsent",
                            "displayFilter": true
                        }
                    }
                ]
            }
        ],
        "methods": [
            {
                "name": "HIDE_CMP"
            },
            {
                "name": "OPEN_OPTIONS"
            },
            {
                "name": "DO_CONSENT"
            },
            {
                "action": {
                    "type": "list",
                    "actions": [
                        {
                            "type": "ifallowall",
                            "trueAction": {
                                "type": "click",
                                "target": {
                                    "selector": "[onclick*='setCookieAcceptType'][onclick*='all']"
                                }
                            },
                            "falseAction": {
                                "type": "click",
                                "target": {
                                    "selector": "[onclick*='setCookieAcceptType'][onclick*='none']"
                                }
                            }
                        },
                        {
                            "type": "click",
                            "target": {
                                "selector": "button[aria-label='Close']"
                            }
                        }
                    ]
                },
                "name": "SAVE_CONSENT"
            },
            {
                "name": "UTILITY"
            }
        ]
    }
}
