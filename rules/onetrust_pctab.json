{
    "$schema": "https://raw.githubusercontent.com/cavi-au/Consent-O-Matic/master/rules.schema.json",
    "onetrust_pctab": {
        "detectors": [
            {
                "presentMatcher": [
                    {
                        "type": "css",
                        "target": {
                            "childFilter": {
                                "target": {
                                    "selector": ".otPcTab"
                                }
                            },
                            "selector": "#onetrust-consent-sdk"
                        }
                    }
                ],
                "showingMatcher": [
                    {
                        "type": "css",
                        "target": {
                            "childFilter": {
                                "target": {
                                    "selector": ".otPcTab",
                                    "displayFilter": true
                                }
                            },
                            "selector": "#onetrust-consent-sdk"
                        }
                    }
                ]
            }
        ],
        "methods": [
            {
                "name": "OPEN_OPTIONS"
            },
            {
                "action": {
                    "type": "foreach",
                    "target": {
                        "selector": ".ot-desc-cntr, .description-container"
                    },
                    "action": {
                        "type": "list",
                        "actions": [
                            {
                                "type": "ifcss",
                                "target": {
                                    "selector": ".ot-grp-hdr1, .group-toggle",
                                    "textFilter": [
                                        "Opbevare og/eller tilgå oplysninger på en enhed",
                                        "Store and/or access information on a device",
                                        "Informationen auf einem Gerät speichern und/oder abrufen",
                                        "Archiviare e/o accedere a informazioni su un dispositivo"
                                    ]
                                },
                                "trueAction": {
                                    "type": "consent",
                                    "consents": [
                                        {
                                            "matcher": {
                                                "type": "checkbox",
                                                "target": {
                                                    "selector": ".ot-grp-hdr1 input, .group-toggle input"
                                                }
                                            },
                                            "toggleAction": {
                                                "type": "click",
                                                "target": {
                                                    "selector": ".ot-grp-hdr1 input, .group-toggle input"
                                                }
                                            },
                                            "type": "D"
